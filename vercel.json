{
    "version":  2,
    "rewrites":  [
                     {
                         "source":  "/docs",
                         "destination":  "/public/docs.html",
                         "has":  [
                                     {
                                         "type":  "host",
                                         "value":  "(?!api\\.).*"
                                     }
                                 ]
                     },
                     {
                         "source":  "/admin",
                         "destination":  "/public/admin.html",
                         "has":  [
                                     {
                                         "type":  "host",
                                         "value":  "(?!api\\.).*"
                                     }
                                 ]
                     },
                     {
                         "source":  "/dashboard",
                         "destination":  "/public/admin.html",
                         "has":  [
                                     {
                                         "type":  "host",
                                         "value":  "(?!api\\.).*"
                                     }
                                 ]
                     },
                     {
                         "source":  "/chat",
                         "destination":  "/public/chatbot.html",
                         "has":  [
                                     {
                                         "type":  "host",
                                         "value":  "(?!api\\.).*"
                                     }
                                 ]
                     },
                     {
                         "source":  "/developer",
                         "destination":  "/public/developer.html",
                         "has":  [
                                     {
                                         "type":  "host",
                                         "value":  "(?!api\\.).*"
                                     }
                                 ]
                     },
                     {
                         "source":  "/dev",
                         "destination":  "/public/developer.html",
                         "has":  [
                                     {
                                         "type":  "host",
                                         "value":  "(?!api\\.).*"
                                     }
                                 ]
                     }
                 ],
    "env":  {
                "OLLAMA_URL":  "https://some-tools-hope.loca.lt",
                "NODE_ENV":  "production",
                "API_DOMAIN":  "api.ajstudioz.dev",
                "MAIN_DOMAIN":  "ajstudioz.dev",
                "LOCALTUNNEL_SUBDOMAIN": "ajstudioz-ai",
                "GROQ_API_KEY": "gsk_placeholder_for_real_groq_key",
                "OPENROUTER_API_KEY": "sk-or-placeholder_for_real_openrouter_key",
                "HUGGINGFACE_API_KEY": "hf_placeholder_for_real_hf_key"
            },
    "headers":  [
                    {
                        "source":  "/api/(.*)",
                        "headers":  [
                                        {
                                            "key":  "Access-Control-Allow-Origin",
                                            "value":  "*"
                                        },
                                        {
                                            "key":  "Access-Control-Allow-Methods",
                                            "value":  "GET, POST, PUT, DELETE, OPTIONS"
                                        },
                                        {
                                            "key":  "Access-Control-Allow-Headers",
                                            "value":  "Content-Type, Authorization, X-API-Key"
                                        },
                                        {
                                            "key":  "X-Powered-By",
                                            "value":  "AJStudioz AI Platform"
                                        },
                                        {
                                            "key":  "X-API-Domain",
                                            "value":  "api.ajstudioz.dev"
                                        }
                                    ]
                    },
                    {
                        "source":  "/(.*)",
                        "has":  [
                                    {
                                        "type":  "host",
                                        "value":  "(?!api\\.).*"
                                    }
                                ],
                        "headers":  [
                                        {
                                            "key":  "X-Frame-Options",
                                            "value":  "DENY"
                                        },
                                        {
                                            "key":  "X-Content-Type-Options",
                                            "value":  "nosniff"
                                        },
                                        {
                                            "key":  "X-Main-Domain",
                                            "value":  "ajstudioz.dev"
                                        }
                                    ]
                    }
                ]
}
