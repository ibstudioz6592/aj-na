# AJ Studios AI API - Environment Variables
# Copy this file to .env and fill in your API keys

# === DATABASE ===
DATABASE_URL=your_neon_database_url_here

# === GITHUB MODELS API (FREE for students!) ===
# Get your token from: https://github.com/settings/tokens/new
# Permissions needed: repo, read:org, read:user
GITHUB_TOKEN=your_github_token_here

# === GROQ API KEYS (5 keys for rate limit management) ===
# Get free keys from: https://console.groq.com/keys
GROQ_API_KEY1=your_groq_api_key_1_here
GROQ_API_KEY2=your_groq_api_key_2_here
GROQ_API_KEY3=your_groq_api_key_3_here
GROQ_API_KEY4=your_groq_api_key_4_here
GROQ_API_KEY5=your_groq_api_key_5_here

# === OPENROUTER API KEYS (5 keys for rate limit management) ===
# Get free keys from: https://openrouter.ai/keys
OPENROUTER_API_KEY1=your_openrouter_api_key_1_here
OPENROUTER_API_KEY2=your_openrouter_api_key_2_here
OPENROUTER_API_KEY3=your_openrouter_api_key_3_here
OPENROUTER_API_KEY4=your_openrouter_api_key_4_here
OPENROUTER_API_KEY5=your_openrouter_api_key_5_here

# === OTHER API KEYS ===
# Chutes AI Token
CHUTES_API_TOKEN=your_chutes_api_token_here

# Cerebras AI Key
CEREBRAS_API_KEY=your_cerebras_api_key_here

# === LOCAL OLLAMA ===
# Default: http://localhost:11434
OLLAMA_URL=http://localhost:11434

# === DEVELOPMENT ===
NODE_ENV=development